    <Actor Name ="SigGen">
      <Command Type="GPIB">
        <Format>SOUR:RAD:AWGN:ARB:BWID {0} MHz</Format>
        <Parameter Prompt="Freq (MHz)" Access="Show">
          <Value>0.0</Value>
